
<!-- 名称核准信息采集 -->
<style>
.gw_cz_box1 table.t1.t2 th{
	width: 17%;
	
}
.gw_cz_box1 table.t1.t2 td{
	width: 32%;
	
}
</style>
<div class="zy_l_box">
	<p class="title">当前执行的任务：$!{taskInfo.getName()}</p>
	<div class="tab_box">
		<table>
			<tr>
				<th width="100px">任务描述：</th>
				<td width="300px">$!{taskInfo.getDescription()}</td>
				<th width="100px">服务编号：</th>
				<td width="300px">$!{taskInfo.getProcessInstanceId()}</td>
			</tr>
			<tr>
				<th>订单号：</th>
				<td>$!{taskOrderId}</td>
				<th>咨询顾问：</th>
				<td>$!{taskInitiator}</td>
			</tr>
			<tr>
				<th>核名状态：</th>
				<td>#if($checkNameStatus)$!{checkNameStatus}#else无#end</td>
				<th></th>
				<td></td>
			</tr>
			<tr>
				<th>客户名称：</th>
				<td>$!{loginEntity.getUsername()}</td>
				<th>客户联系电话：</th>
				<td>$!{loginEntity.getUserphone()}</td>
			</tr>
			#if($checkNameTuiHuiReason && $!{checkNameTuiHuiReason} != "" )
			<tr>
				<th>退回原因：</th>
				<td>$!{checkNameTuiHuiReason}</td>
				<th>退回描述：</th>
				<td>$!{checkNameTuiHuiRemark}</td>
			</tr>
			#end
		</table>
	</div>
	<div class="zy_btn">
		 <a class="b3">信息采集</a>
	</div>
</div>
<script type="text/javascript">
	$(document).ready(function($) {
		// 点击 - 信息采集
		$(".b3").click(function() {
		    var url = "/enterprise/emp/edit/heMing-v5?orderId=" + orderId + "&sourceType=2&morePage=false";
		    window.open(url,'','width='+(window.screen.availWidth-10)+',height='+(window.screen.availHeight-30)+ ', top=0, left=0, toolbar=no, menubar=no, scrollbars=no, resizable=no, location=no, status=no');
		});
	});
</script>

