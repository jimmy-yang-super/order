<!-- 取得全部证照归档 -->

<div class="zy_l_box">
	<p class="title">当前执行的任务：$!{taskInfo.getName()}</p>
	<div class="tab_box">
		<table>
			<tr>
				<th width="200px">任务描述：</th>
				<td width="300px">$!{taskInfo.getDescription()}</td>
				<th width="200px">服务编号：</th>
				<td width="300px">$!{taskInfo.getProcessInstanceId()}</td>
			</tr>
			<tr>
				<th >产权主体：</th>
				<td >$!{reginAddress.getChanqzt()}</td>
				<th >注册地址：</th>
				<td >$!{iszhucdz}</td>
			</tr>
			<tr>
				<th >申请方式：</th>
				<td >$!{applyMethod}</td>
				<th >档案位置编号：</th>
				<td >$!{createArchiveID}</td>
			</tr>
			<tr>
				<th >提档人：</th>
				<td >$!{WFUtils.getAssgineeByProcessInstanceIdAndTaskDefId($taskInfo.getProcessInstanceId(), "ReceiveAllLicense")}</td>
				<th >提取内容：</th>
				<td >工商申请材料</td>
			</tr>
		</table>
	</div>
	<div class="zy_btn">
		<a class="b1">确认归档</a>
	</div>
</div>
<script type="text/javascript">
	$(document).ready(function($) {
		
		/**** 确认存放 *****************************/
		// 点击 - 确认存放
		$(".b1").click(function() {
			// 解除 click 事件，防止二次点击
			$(this).unbind("click");
			WF.submitTask("111", "&ReceiveAllLicenseDataStatus=2");
		});
	});
</script>