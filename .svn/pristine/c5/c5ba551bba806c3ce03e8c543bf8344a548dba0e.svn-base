
body,p,h3,h4,dl,dd{margin:0}
ul{margin:0;padding:0;}
input{margin:0;padding:0;outline:0;border:0;}
textarea{outline:none}
ul{list-style:none;}
h3,h4{font-size:100%;font-weight:normal}
body{font-family:"微软雅黑",Arial,SimSun,Helvetica,sans-serif;font-size:14px;color:#585757;}
a{color:#1155CC;text-decoration:none}
