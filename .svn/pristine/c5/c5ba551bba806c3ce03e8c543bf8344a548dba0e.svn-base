.dataCon dd a.oper {display:block;line-height:50px;border:1px solid #FFF;padding:0 5px;float:left;}
.dataCon dd a.oper:first-child {padding-left:18px;}
.dataCon dd.aOperDD a.oper:first-child {padding-left:8px;}
.dataCon dl:nth-child(2n) dd a.oper{border-color:#FFF;} 
.dataCon dl:nth-child(2n+1) dd a.oper{border-color:#E5F5FF;} 
.dataCon dl dd a.sel{border-color:#D3D3D3!important;border-top-color:#228BCC!important;border-bottom-color:#F5F5F5!important;background:#F5F5F5;font-weight:bold;position:relative;}
.dataCon dl dd.operDD a.sel{border-width:1px 0;}
.dataCon dl dd a.sel:after{content:'';display:block;height:1px;width:100%;background:#F5F5F5;position:absolute;bottom:-2px;left:0;z-index:12;}
.dataCon dd.ddDetail,.dataCon dd.jzDetail,.dataCon dd.jlDeatil{clear:both;width:100%;background:#F5F5F5;text-align:left;padding:0 15px;box-sizing:border-box;position:relative;border-top:1px solid #D3D3D3;border-bottom:1px solid #D3D3D3;margin-bottom:5px;   display:none;}
