.dataCon {width:980px;border:1px solid #CECECE;margin:15px auto 0;padding-bottom:60px;position:relative;z-index:1;}
.dataCon .dataTle{background:#F0F0F0!important;font-size:14px!important;}
.dataCon dl:nth-child(2n){background:#FFFFFF;}
.dataCon dl:nth-child(2n+1){background:#E5F5FF;}
.dataCon dl:after{clear:both;content:'';display:table;}
.dataCon dl{text-align:center;line-height:52px;font-size:12px;}
.dataCon dl.done{color:#969696;}
.dataCon dl.done .f-red,.dataCon dl.done .f-yellow,.dataCon dl.done a{color:#969696;}
.dataCon dt{float:left;display:table;vertical-align:middle;width:100px;}
.yqddBody .dataCon dt{width:110px;}
.dataCon dd.pr{position:relative;cursor:pointer;}
.dataCon dd i.down,.dataCon dd i.up{display:block;height:8px;width:8px;border:1px solid #323232;border-width:1px 1px 0 0;-webkit-transform:rotate(135deg);position:absolute;right:20px;top:25px;}
.qzddBody .dataCon dd i.down,.qzddBody .dataCon dd i.up{right:2px;}
.dataCon dd i.down{-webkit-transform:rotate(-45deg);top:22px;}
.dataCon dd .pCon,.dataCon dd .pCon_3{padding:8px 0;}
.dataCon dd .pCon_1 p{line-height:52px;}
.dataCon dd .pCon_3 p{line-height:12px;}
.dataCon dd p {line-height:18px;width:100%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;min-height:3px;}
.dataCon dd p .xgIcon{display:block;}
.dataCon dd .timeCon{padding:9px;}
.dataCon .timeCon p{height:34px;white-space:normal;}
.dataCon dd .f-black{color:#323232;}
.dataCon dd .f-red{color:#C21F1F;}
.dataCon dd{float:left;display:table-cell;vertical-align:middle;width:80px;min-height:30px;}
.dataCon dl dd.operDD{position:relative;}
.yqddBody .dataCon dl dd.operDD{position:relative;width:100px;}
.dataCon dd.w90{width:90px;}
.dataCon dd.w90 a.oper{width:40px;margin-left:25px;}
.dataCon dd.w100{width:100px;}
.dataCon dd.w140{width:140px;}
