.fqWin i.colseIcon:after{content:'+';display:block;color:#E2E2E2;font-size:44px;-webkit-transform:rotate(45deg);position:absolute;left:0;top:-18px;}
.fqWin p{line-height:26px;padding-bottom:15px;}
.fqWin textarea{height:130px; width:400px;padding:5px;}
.fqWin div{ height:50px;padding-top:20px;}
.fqWin div .subBtn{ float:right;}

/*addWin*/
.addWin{position:absolute;top:150px;left:60px;width:960px;box-shadow:0 0 15px #999 ;z-index:999;background:#FFF;display:none;padding-bottom:28px;}
.addWin .wLCon{float:left;padding:20px 0 0 30px;width:450px;}
.addWin .vHr{height:75%;top:15px;left:470px;}
.addWin li{line-height:26px;clear:both;margin-bottom:10px;}
.addWin .wLCon li{margin-bottom:17px;}
.addWin li:after{content:'';display:table;clear:both;}
.addWin li>span{float:left;padding-right:5px;}
.addWin li .infoInput{height:26px;line-height:26px;padding-left:5px;border:1px solid #D0D0D0;float:left;}
.addWin li .infoInput.b-red{border-color:#EE2200;}
.addWin li .infoInput input{height:22px;line-height:22px;}
.addWin li.moneyLi{ float:left;width:220px;clear:none;}
.addWin li.moneyLi input{color:#C73333;width:100px;font-weight:bold;}
.addWin li.moneyLi span:last-child{float:left;padding-left:5px;}

.addWin .wRCon{float:left;width:440px;padding:20px;}
.addWin .iData{padding-left:0;float:none;}
.addWin .iData h4{padding:0;clear:both;}
.addWin .iData .yxSel {padding-top:15px;}
.addWin .iData .yxSel a.sel {height:22px;line-height:22px;}
.addWin .operBtn{border-top:1px solid #C8C8C8;clear:both;text-align:center;padding:20px 0 5px;position:relative;top:8px;}
.addWin .operBtn a{display:inline-block;}
.addWin .operBtn a.canBtn{margin-left:30px;}
#yewuDtil{display:block;width:370px;border-top:1px solid #D0D0D0;padding:0 6px;top:27px;background:#FFF;}
#yewuDtil .selCon{border-bottom:2px solid #EFEFEF;padding:7px 0;}
#yewuDtil .selCon:last-child{border:0;}
#yewuDtil .selCon:after{clear:both;content:'';display:table;}
#yewuDtil .selCon>p{float:left;width:72px;line-height:18px;}
#yewuDtil .selCon ul{float:left;width:255px;}
#yewuDtil .selCon ul li{height:18px;line-height:18px;border:0;float:left;padding:0 10px;margin:0 3px 0 0;clear:none;border:1px solid #FFF;margin-bottom:3px;}
#yewuDtil .selCon ul li:hover{background:#FFF;}
#yewuDtil .selCon ul li.sel{border-color:#D94242;color:#D94242}

/*timeDiv*/
.timeDiv {width:210px;border:1px solid #DDDDDD;padding:7px 5px 10px;background:#fff;color:#282828;position:absolute;top:28px;display:none;}
#sTimeDiv{left:-1px;}
#eTimeDiv{left:110px;}
.timeDiv p.tTle{line-height:24px;position:relative;text-align:center;padding-bottom:5px;}
.timeDiv p.tTle i{display:block;position:absolute;height:20px;width:20px;border-radius:2px;top:1px;cursor:pointer;}
.timeDiv p.tTle i:after{content:'';height:8px;width:8px;border:1px solid #999999;display:block;border-width:1px 1px 0 0;left:5px;top:5px;position:absolute;}
.timeDiv p.tTle i.prev{left:10px;}
.timeDiv p.tTle i.next{right:10px;}
.timeDiv p.tTle i.prev:after{-webkit-transform:rotate(-135deg);}
.timeDiv p.tTle i.next:after{-webkit-transform:rotate(45deg);}
.timeDiv p.tTle i:hover{background:#E6E6E6;}
.indexTle li{color:#333333;font-weight:bold;border-radius:2px;}
.timeDiv ul:after{clear:both;display:table;content:'';}
.timeDiv li{float:left;width:30px;text-align:center;line-height:22px;margin-bottom:3px;}
.timeDiv li:nth-child(7n+1){color:#EE2200;}
.timeDiv li:nth-child(7n){color:#EE2200;}
.timeDiv li.oMonth{color:#888888;cursor:default;}
.timeDiv li.oMonth:hover{background:#FFF;}
.timeDiv .timeCon li:hover{background:#E6E6E6;cursor:pointer;}
.timeDiv li.sel{background:#2358A0;color:#FFF;}
.timeDiv li.sel:hover{background:#2358A0;}
.timeDiv .indexTle li{background:#BDEBEE;color:#333333!important;}

