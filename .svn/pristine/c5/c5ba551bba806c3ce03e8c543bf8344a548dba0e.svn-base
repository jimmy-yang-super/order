/*yqddBody*/
.yqddBody,.qzddBody	{width:1028px;margin:0 auto;border:0;padding-bottom:30px;}
header{height:32px;line-height:32px;width:1000px;margin:0 auto;border-bottom:1px solid #D7D7D7;padding-top:15px;padding-left:20px;margin-bottom:15px;}
header h3{font-weight:bold;color:#323232;font-size:16px;height:30px;line-height:30px;}
header h3 p {float:left;display:inline-block;padding:0 10px;cursor:pointer;}
header h3 p.cur {border-bottom:2px solid #15589E;}
.yqddBody nav{width:980px;border:1px solid #E6E6E6;margin:0  auto 15px;text-align:center;}
.yqddBody nav ul:after{display:table;content:'';clear:both;}
.yqddBody nav .key{background:#F0F0F0;color:#323232;font-size:14px;}
.yqddBody nav .value{font-size:32px;font-weight:bold;}
.yqddBody nav li {box-sizing:border-box;width:33%;float:left;}
.yqddBody nav li:last-child{width:34%;}
.yqddBody nav .key li{line-height:36px;}
.yqddBody nav .value li{line-height:64px;color:#464646;}
.yqddBody nav .value li:first-child{color:#26358E;border-right:1px solid #DDDDDD;}
.yqddBody nav .value li:last-child{color:#C21F1F;border-left:1px solid #DDDDDD;}
.selSear{clear:both;width:980px;margin:0 auto;position:relative;z-index:9}
.selSear:after{clear:both;content:'';display:table;}
.selType{border:1px solid #D0D0D0; float:left;height:26px;line-height:26px;color:#323232;padding-left:8px;position:relative;margin-right:15px;cursor:pointer;}
.selType i.down,.selType i.up{display:block;height:10px;width:10px;border:2px solid #BFBFBF;border-width:2px 2px 0 0;-webkit-transform:rotate(135deg);position:absolute;right:10px;top:6px;cursor:pointer;}
.selType i.up{-webkit-transform:rotate(-45deg);top:11px;}
.selType .selDtil{position:absolute;left:-1px;top:26px;border:1px solid #D0D0D0;box-sizing:border-box;border-top:0;z-index:1099;background:#FFF;display:none;}

.selDtil li{border-bottom:1px solid #E7E7E7;line-height:26px;cursor:pointer;padding:2px 7px;}
.selDtil li:hover{background:#EEEEEE;}
.selDtil li:last-child{border:0;}
