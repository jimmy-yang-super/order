function savegsmc(){
	var orderid = $("#orderid").val();
	var type =$("#addgsmc").val();
	var comptype =$("input[name='comptype']:checked").val();
	var gongsmc = $("#gongsmc").val();
	var pattern=/^[\u4e00-\u9fa5]+$/; 
	if(gongsmc == null || gongsmc ==""){
		alert("请输入公司名称！！");
		return false;
	}else{
		//验证必须为汉字
		 if (!pattern.test(gongsmc))
		 { 
		    alert("输入公司名称必须为汉字。"); 
		    return false; 
		 }
	}
	var beixmc ="";
	for(var i =1 ;i<=beixcount;i++){
		if($("#beixmc"+i).val() != null && $("#beixmc"+i).val() !=""){
			 if (!pattern.test($("#beixmc"+i).val()))
			 { 
			    alert("输入公司备选名称必须为汉字。"); 
			    return false; 
			 }
			beixmc = beixmc +$("#beixmc"+i).val()+"&";
		}
	}
	if (beixmc != "") {
		beixmc = beixmc.substring(0, beixmc.lastIndexOf("&"));
	}
	var zuczb = $("#zuczb").val();
	var busid = $("#busid").val();
	if(zuczb == null || zuczb ==""){
		alert("请输入公司注册资本！！");
		return false;
	}else{
		var type1="^\\d+$" ; 
	    var re   =   new   RegExp(type1); 
		if(zuczb.match(re)==null){
			alert("输入的公司注册资本必须为整数！！");
			return false;
		}
	}
	 jQuery.ajax({
		url : '/order/addBusinessinfo',
		type : "post",
		data : {
			"type" : type,
			"comptype" : comptype,
			"gongsmc" : gongsmc,
			"beixmc" : beixmc,
			"zuczb" : zuczb,
			"orderid":orderid,
			"busid":busid
		},
		dataType : "text",
		success : function(data, textStatus) {
			if(data !="" && data !="0"){
				$("#busid").val(data);
				 alert("保存成功!!");
			}
		},
		error : function(XMLHttpRequest, textStatus, errorThrown) {
		}
	}); 
}
