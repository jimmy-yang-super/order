<!-- 确认支出地址费用 -->

<div class="zy_l_box">
	<p class="title">当前执行的任务：$!{taskInfo.getName()}</p>
	<div class="tab_box">
		<table>
			<tr>
				<th width="200px">任务描述：</th>
				<td width="300px">$!{taskInfo.getDescription()}</td>
				<th width="200px">服务编号：</th>
				<td width="300px">$!{taskInfo.getProcessInstanceId()}</td>
			</tr>
			<tr>
				<th>地址类型：</th>
				<td >$!{addressType}</td>
				<th>订单号：</th>
				<td>$!{taskOrderId}</td>
			</tr>
			<tr>
				<th>申请支付金额：</th>
				<td>$!{applyAddressFeeFuKuanJinE}</td>
				<th>地址合作商：</th>
				<td>$!{addressPartner}</td>
			</tr>
		</table>
	</div>
	<div class="zy_btn">
		<a class="b1" >确认支出</a>
	</div>
</div>
<script type="text/javascript">
$(document).ready(function($){
	/************ 确认支出 *************************/
	// 点击 - 确认支出
	$(".b1").click(function(){
		if(!WF.validate()){
			return;
		}
		// 解除 click 事件，防止二次点击
		$(this).unbind("click");
		
		WF.submitTask();
		WF.taskFinish();
	});
});

</script>