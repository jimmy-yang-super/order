.cue_ceng ul li div.t1 p span{width: 60px;display: inline-block;}
.cue_ceng ul li div.t1 p span.s1{height: 22px;line-height: 22px;}
.cue_ceng ul li div.t1 p span.s2{height: 20px;line-height: 20px;color: #fff}
.cue_ceng ul li div.t1 p.red{border: 1px solid #fe8988;background: #fff url(/images/enterprise/c_ico1.png) no-repeat center bottom;}
.cue_ceng ul li div.t1 p.blue{border: 1px solid #3bb7e8;background: #fff url(/images/enterprise/c_ico2.png) no-repeat center bottom;}
.cue_ceng ul li div.t1 p.green{border: 1px solid #9dcd29;background: #fff url(/images/enterprise/c_ico3.png) no-repeat center bottom;}
.cue_ceng ul li div.t1 p.yellow{border: 1px solid #fdb01a;background: #fff url(/images/enterprise/c_ico4.png) no-repeat center bottom;}
.cue_ceng ul li div.name2{width: 70px;float: left;font-size: 12px;color: #333;height: 20px;line-height: 20px}
.cue_ceng ul li div.t2{float: left;width:280px;height: 20px;color: #fff}
