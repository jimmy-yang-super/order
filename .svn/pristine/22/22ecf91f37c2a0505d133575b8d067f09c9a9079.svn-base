.cue_ceng{width: 350px;min-height: 140px; padding: 5px 15px; border: 1px solid #ffc600; background: #fff8e0; position: absolute; z-index: 9999; right: 15px; top:0px; display: none;  -moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius: 5px;}
.cue_ceng .angle {position:absolute;top:17px;*top:7px;left:-21px;display:block;height:0;width:0;font-size: 0; line-height: 0;border-color:transparent #ffc600 transparent  transparent;border-style:dashed solid dashed  dashed;border-width:10px;}
.cue_ceng .angle s{position:absolute;top:-10px;*top:-10px;left:-9px;display:block;height:0;width:0;font-size: 0;line-height: 0;border-color:transparent #fff8e0 transparent  transparent;border-style:dashed solid dashed  dashed;border-width:10px;}
.cue_ceng ul{overflow: hidden;}
.cue_ceng ul li{padding: 15px 0; border-bottom: 1px solid #e5dfc9;overflow: hidden;}
.cue_ceng ul li:last-child{border-bottom: none;}
.cue_ceng ul li div.name1{width: 70px;float: left;font-size: 12px;color: #333;height: 45px;line-height: 45px}
.cue_ceng ul li div.t1{float: left;width:280px;height: 45px;}
