$(function(){
  //设置奇数行背景色
  $("#tab tr:odd").find("td").addClass("tr_odd");
  //设置偶数行背景色
  $("#tab tr:even").find("td").addClass("tr_even");
  // 鼠标移到的颜色
  $("#tab tr").mouseover(function(){
    $(this).find("td").addClass("mouse_color");
  });
  // 鼠标移出的颜色
  $("#tab tr").mouseout(function(){
    $(this).find("td").removeClass("mouse_color");
  });
});
$(function(){
