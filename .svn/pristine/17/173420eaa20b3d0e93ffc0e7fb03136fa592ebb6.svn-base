
<!-- 自动确认 -->
<div class="zy_l_box">
	<p class="title">当前执行的任务：$!{taskInfo.getName()}</p>
	<div class="tab_box">
		<table>
			<tr>
				<th width="210px">任务描述：</th>
				<td width="300px">$!{taskInfo.getDescription()}</td>
				<th width="220px">服务编号：</th>
				<td width="300px">$!{taskInfo.getProcessInstanceId()}</td>
			</tr>
		</table>
	</div>
	<div class="zy_btn">
		<a class="b1" >地税报到材料准备</a> <a class="b2">地税报到信息采集(建档)</a>
		<a class="b3">地税报到信息采集(归档)</a>
	</div>
</div>
<script type="text/javascript">
$(document).ready(function($){
	
	/************ 地税报到材料准备*************************/
	// 点击 - 地税报到材料准备
	$(".b1").click(function(){
		// 解除 click 事件，防止二次点击
		$(this).unbind("click");
		// 调用流程接口
		var taskData = "&stuff_off=1&createArchiveStatus=1";
		WF.submitTask("", taskData);
		/* WF.showDiv('yiHeZhunDiv'); */
	});
	
	
	/************ 地税报到信息采集 *************************/
	// 点击 - 地税报到信息采集
	$(".b2").click(function(){
		// 解除 click 事件，防止二次点击
		$(this).unbind("click");
		// 调用流程接口
		var taskData = "&stuff_off=0&createArchiveStatus=0";
		WF.submitTask("",taskData);
	});
	// 点击 - 地税报到信息采集
	$(".b3").click(function(){
		// 解除 click 事件，防止二次点击
		$(this).unbind("click");
		// 调用流程接口
		var taskData = "&stuff_off=0&createArchiveStatus=1";
		WF.submitTask("",taskData);
	});
});

</script>
